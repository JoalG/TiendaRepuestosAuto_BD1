@model IEnumerable<TiendaRepuestosAuto_BD1_WEB.Models.Organizacion>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Cédula jurídica")
        </th>
        <th>
            @Html.DisplayName("Nombre de Organización")
        </th>
        <th>
            @Html.DisplayName("Ciudad")
        </th>
        <th>
            @Html.DisplayName("Dirección")
        </th>
        <th>
            @Html.DisplayName("Estado")
        </th>
        <th>
            @Html.DisplayName("Contacto")
        </th>
        <th>
            @Html.DisplayName("Telefono")
        </th>
        <th>
            @Html.DisplayName("Cargo")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CedulaJuridica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cliente.Ciudad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cliente.Direccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cliente.EstadoDeCliente.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contacto.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contacto.Telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contacto.Cargo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CedulaJuridica }) |
            @Html.ActionLink("Details", "Details", new { id=item.CedulaJuridica }) |
            @Html.ActionLink("Suspender", "suspenderOrganizacion", new { id=item.CedulaJuridica })
        </td>
    </tr>
}

</table>
